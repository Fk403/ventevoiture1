{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "button-type": "off",
    "no-inline-styles": "off",
    "detect-css-reflows/composite": "off",
    "axe/structure": [
      "default",
      {
        "list": "off",
        "listitem": "off"
      }
    ],
    "axe/parsing": [
      "default",
      {
        "duplicate-id": "off",
        "duplicate-id-active": "off",
        "duplicate-id-aria": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "select-name": "off",
        "label": "off"
      }
    ],
    "axe/aria": [
      "default",
      {
        "aria-required-children": "off",
        "aria-required-parent": "off"
      }
    ],
    "disown-opener": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-tap-highlight-color"
        ]
      }
    ]
  }
}